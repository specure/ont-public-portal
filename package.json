  "name": "ont-public-portal",
  "version": "3.1.2",
  "scripts": {
    "configure-playwright-dev": "cp -f ./src/environments/environment.ts ./src/environments/environment.playwright.ts",
    "configure-playwright-stage": "cp -f ./src/environments/environment.stage.ts ./src/environments/environment.playwright.ts",
    "configure": "node ./scripts/configure-flavor",
    "ng": "ng",
    "start": "ng serve",
    "start-local": "ng serve --configuration=local --poll=2000",
    "start-stage": "ng serve --configuration=stage --poll=2000",
    "start-prod": "ng serve --configuration production --poll=2000",
    "e2e": "npm run configure nkom && npm run configure-playwright-dev && start-server-and-test http-get://localhost:4200 \"npm run test\"",
    "e2e-headless": "WAIT_ON_INTERVAL=300000 npm run configure nkom && npm run configure-playwright-dev && start-server-and-test http-get://localhost:4200 \"npm run test-headless\"",
    "e2e-local": "npm run configure-playwright-dev && start-server-and-test http-get://localhost:4200 \"npm run test\"",
    "e2e-stage": "npm run configure nkom && npm run playwright-playwright-dev && start-server-and-test \"npm run start-stage\" http-get://localhost:4200 \"npm run test\"",
    "e2e-stage-headless": "WAIT_ON_INTERVAL=300000 npm run configure nkom && npm run configure-playwright-stage && start-server-and-test \"npm run start-stage\" http-get://localhost:4200 \"npm run test-headless\"",
    "build": "ng build",
    "build-dev": "ng build --configuration=dev",
    "build-prod": "ng build --configuration production",
    "test": "npx playwright test --config=test/config/playwright.config.ts",
    "test-headless": "npx playwright test --config=test/config/playwright-headless.config.ts",
    "lint": "ng lint",
    "prepare": "husky install",
    "dev:ssr": "ng run nettest:serve-ssr",
    "serve:ssr": "node dist/nettest/server/main.js",
    "build:ssr": "ng build && ng run nettest:server",
    "prerender": "ng run nettest:prerender --no-guess-routes --routes-file src/prerender-routes.txt",
    "prerender:dev": "node ./scripts/prerender-routes.js --env dev && npm run prerender -- --browser-target='nettest:build:dev' --server-target='nettest:server:development'",
    "prerender:beta": "node ./scripts/prerender-routes.js --env beta && npm run prerender -- --browser-target='nettest:build:beta' --server-target='nettest:server:beta'",
    "prerender:prod": "node ./scripts/prerender-routes.js --env prod && npm run prerender",
    "postinstall": "npm update @nettest/cookie-widget"
  },
  "private": true,
